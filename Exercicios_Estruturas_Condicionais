        "# 11. Uma empresa resolveu dar um aumento de salário aos seus colaboradores\n",
        "# e lhe contrataram para desenvolver o programa que calculará os reajustes. Faça\n",
        "# um programa que recebe o salário de um colaborador e o reajuste segundo o\n",
        "# seguinte critério, baseado no salário atual:\n",
        "\n",
        "# salários até R$ 280,00 (incluindo): aumento de 20%\n",
        "# salários entre R$ 280,00 e R$ 700,00: aumento de 15%\n",
        "# salários entre R$ 700,00 e R$ 1500,00: aumento de 10%\n",
        "# salários de R$ 1500,00 em diante: aumento de 5%\n",
        "\n",
        "# Após o aumento ser realizado, informe na tela:\n",
        "# o salário antes do reajuste;\n",
        "# o percentual de aumento aplicado;\n",
        "# o valor do aumento;\n",
        "# o novo salário, após o aumento.\n",
        "\n",
        "salario = float(input(\"Digite o salário: \"))\n",
        "\n",
        "if salario < 280:\n",
        "  print(f\"O salário era R$ {salario:.2f}\")\n",
        "  print(\"O percentual de aumento foi de 20%\")\n",
        "  print(f\"O valor do aumento foi R$ {salario * (20/100):.2f}\")\n",
        "  print(f\"O novo salário é R$ {salario * (20/100) + salario:.2f}\")\n",
        "\n",
        "elif salario >= 280 and salario < 700:\n",
        "  print(f\"O salário era R$ {salario:.2f}\")\n",
        "  print(\"O percentual de aumento foi de 15%\")\n",
        "  print(f\"O valor do aumento foi R$ {salario * (15/100):.2f}\")\n",
        "  print(f\"O novo salário é R$ {salario * (15/100) + salario:.2f}\")\n",
        "\n",
        "elif salario >= 700 and salario < 1500:\n",
        "  print(f\"O salário era R$ {salario:.2f}\")\n",
        "  print(\"O percentual de aumento foi de 10%\")\n",
        "  print(f\"O valor do aumento foi R$ {salario * (10/100):.2f}\")\n",
        "  print(f\"O novo salário é R$ {salario * (10/100) + salario:.2f}\")\n",
        "\n",
        "else:\n",
        "  print(f\"O salário era R$ {salario:.2f}\")\n",
        "  print(\"O percentual de aumento foi de 5%\")\n",
        "  print(f\"O valor do aumento foi R$ {salario * (5/100):.2f}\")\n",
        "  print(f\"O novo salário é R$ {salario * (5/100) + salario:.2f}\")\n"
